                                    <div style="color:#fff; font-size:10px; font-weight:400; line-height:12px;">
                                    +55 (61) 3030-5700
                                    </div>
                                </td>
                            </tr>
                        </mj-table>
                    </mj-column>
                    <mj-column width="14.5%">
                    </mj-column>
                </mj-group>
            </mj-section>

            <mj-section padding="10px 0px"
            
            >
                <mj-group>
                    <mj-column width="14.5%">
                    </mj-column>
                    <mj-column width="71.0%">
                        <mj-table align="left" width="90px" padding="0">
                            <tr>
                                <td width="30px" style="padding:0 0px 0 0;">
                                    <img src="https://lacunalrs.blob.core.windows.net/public/email-assets/email-de-fim-de-ano-2025/icones/Facebook.png" width="23" />
                                </td>
                                <td width="30px" style="padding:0 10px;">
                                    <img src="https://lacunalrs.blob.core.windows.net/public/email-assets/email-de-fim-de-ano-2025/icones/Instagram.png" width="23" />
                                </td>
                                <td width="30px" style="padding:0 0 0 0px;">
                                    <img src="https://lacunalrs.blob.core.windows.net/public/email-assets/email-de-fim-de-ano-2025/icones/Linkidin.png" width="23" />
