                        />

                        
                        <mj-image
                            src="https://lacunalrs.blob.core.windows.net/public/email-assets/email-de-fim-de-ano-2025/Globo.png"
                            height="56px"
                            width="56px"
                            padding="0" 
                            css-class="img-smaller"
